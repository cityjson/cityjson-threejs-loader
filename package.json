{
  "name": "cityjson-threejs-loader",
  "version": "0.3.1",
  "description": "A CityJSON loader for three.js",
  "repository": "http://github.com/cityjson/cityjson-threejs-loader",
  "author": "Stelios Vitalis",
  "license": "MIT",
  "type": "module",
  "main": "src/index.js",
  "files": [
    "src/*"
  ],
  "scripts": {
    "start": "parcel example/*.html",
    "dev": "concurrently \"parcel watch example/*.html --out-dir ./example/dev-bundle/ --public-url . --no-cache\" \"static-server\"",
    "lint": "eslint \"src/**/*.js\""
  },
  "dependencies": {
    "earcut": "^2.2.2"
  },
  "peerDependencies": {
    "three": ">0.139.2"
  },
  "devDependencies": {
    "@types/three": ">0.139.2",
    "concurrently": "^6.0.0",
    "eslint": "^7.18.0",
    "eslint-config-mdcs": "^5.0.0",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "regenerator-runtime": "^0.13.9",
    "static-server": "^2.2.1",
    "parcel": "^2.9.3"
  }
}
