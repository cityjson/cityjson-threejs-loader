{
  "name": "cityjson-threejs-loader",
  "version": "0.4.0",
  "description": "A CityJSON loader for three.js",
  "repository": "http://github.com/cityjson/cityjson-threejs-loader",
  "author": "Stelios Vitalis",
  "license": "MIT",
  "type": "module",
  "main": "src/index.js",
  "files": [
    "src/*"
  ],
  "targets": {
    "main": {
      "source": "src/index.js",
      "distDir": "./dist"
    },
    "examples": {
      "source": "example/*.html",
      "distDir": "./dist-examples"
    }
  },
  "scripts": {
    "start": "parcel serve --target examples",
    "build": "parcel build --target main",
    "lint": "eslint \"src/**/*.js\" \"example/**/*.js\"",
    "lint:fix": "eslint \"src/**/*.js\" \"example/**/*.js\" --fix"
  },
  "dependencies": {
    "@cityjson/cjseq": "^0.0.1",
    "earcut": "^2.2.2"
  },
  "peerDependencies": {
    "three": "^0.165.0"
  },
  "devDependencies": {
    "@types/three": "^0.165.0",
    "concurrently": "^6.0.0",
    "eslint": "^7.18.0",
    "eslint-config-mdcs": "^5.0.0",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "parcel": "^2.9.3",
    "parcel-reporter-static-files-copy": "^1.5.0",
    "regenerator-runtime": "^0.13.9",
    "static-server": "^2.2.1"
  }
}